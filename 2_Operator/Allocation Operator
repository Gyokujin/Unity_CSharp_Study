using System.Collections;
using System.Collections.Generic;
using UnityEngine;

public class AllocationOperator : MonoBehaviour
{
    void Start()
    {
        int a = 5;
        int b = 3;
        int c = 4;

        // 할당 연산
        a = b + c;
        a = a + 2;
        a += 2;
        a *= 2;
        a -= 2;
        a /= 2;
        
        Debug.Log(a);

        a++; // a += 1
        a--; // a -= 1

        Debug.Log(a);
    }
}
