using System.Collections;
using System.Collections.Generic;
using UnityEngine;

public class TrashBin : Box
{
    public override void Introduce() // 부모 클래스인 Box에서 virtual 선언하였기 때문에 override 가능하다.
    {
        base.Introduce(); // base로 부모 클래스에 다시 접근이 가능하다.
        Debug.Log("안녕하세요 전 쓰레기통인데요?");
    }
}
