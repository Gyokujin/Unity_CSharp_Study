using System.Collections;
using System.Collections.Generic;
using UnityEngine;

public class MultidimensionalArray : MonoBehaviour
{
    void Start()
    {
        // 다차원 배열
        int[,] a = new int[4, 6];

        for (int i = 0; i < 4; i++)
        {
            for (int j = 0; j < 6; j++)
            {
                a[i, j] = i * 6 + j;
                Debug.Log("i" + i + ", j" + j + " = " + a[i, j]);
            }
        }
    }
}
